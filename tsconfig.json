{
	"compilerOptions": {
		"incremental": true,
		"target": "es5",
		"module": "commonjs",
		"outDir": "./",
		"rootDir": "./src",
		"tsBuildInfoFile": "./.tsbuildinfo",
		"strict": true,
		"noImplicitAny": false,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"lib": ["dom", "ES2015", "esnext"]
	},
	"include": ["./src/**/*.ts"],
	"exclude": ["./node_modules"]
}
