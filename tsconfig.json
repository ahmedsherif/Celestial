{
	"compilerOptions": {
		"incremental": true,
		"target": "es5",
		"module": "commonjs",
		"outDir": "./",
		"rootDir": "./src",
		"tsBuildInfoFile": "./",
		"strict": true,
		"noImplicitAny": false,
		"baseUrl": ".",
		"paths": {
			"@app/*": ["src/*"]
		},
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}
